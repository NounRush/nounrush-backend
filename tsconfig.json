{
    "compilerOptions": {
      "target": "ES2020",
      "module": "commonjs",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "resolveJsonModule": true
    },
    "include": [
      "./src/**/**/*.ts",
      "./src/**/*.ts",
      "src/server.ts"
    ],
    "exclude": [
      "./dist"
    ]
}
